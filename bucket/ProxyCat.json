        "checkver": {
            "url": "https://api.github.com/repos/honmashironeko/ProxyCat/releases/latest",
            "jsonpath": "$.tag_name",
            "regex": "V(.*)"
        },
    "suggest": {
        "python": "python"
    },
    "url": "https://github.com/honmashironeko/ProxyCat/releases/download/ProxyCat-V1.9/ProxyCat-V1.9.zip",
    "extract_dir": "ProxyCat-main/ProxyCat-CN",
    "hash": "c3a926768bda9026e658ca89a442378ae9896e34951ee191be19f00c2d36e2d5",
    "pre_install": [
        "$content = (Get-Content -Path $(Join-Path $(Find-BucketDirectory -Root -Name ctftools) starters/ProxyCat.cmd)) -replace '%%HOME%%', $dir",
        "Set-Content -Path $(Join-Path $dir ProxyCat.cmd) -Value $content -Encoding UTF8",
        "pip install -r (Join-Path $dir \"requirements.txt\")",
        "Move-Item -Path $(Join-Path $dir ProxyCat*.py) -Destination $(Join-Path $dir ProxyCat.py) -Force"
    ],
    "autoupdate": {
        "url": "https://github.com/honmashironeko/ProxyCat/releases/download/ProxyCat-V$version/ProxyCat-V$version.zip"
    },
    "bin": [
        [
            "ProxyCat.cmd",
            "ProxyCat"
